  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch site",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/main.ts",
      "args": [ "dev" ],
      "skipFiles": [ "<node_internals>/**" ],
      "runtimeArgs": [ "--watch-path=main.ts", "--watch-path=static.ts", "--disable-warning=ExperimentalWarning" ]
    }
  ]
}
